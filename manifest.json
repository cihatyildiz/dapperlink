{
    "manifest_version": 3,
    "name": "Custom URL Shortener",
    "description": "Shortens URLs for quick navigation",
    "version": "1.0",
    "permissions": ["webRequest", "webRequestBlocking", "<all_urls>", "storage"],
    "background": {
        "service_worker": "background.js"
    },
    "options_page": "options.html",
    "action": {
        "default_icon": {
            "48": "icon.png"
        },
        "default_popup": ""
    }
}
